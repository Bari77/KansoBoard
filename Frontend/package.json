{
    "name": "kansoboard",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "ng": "ng",
        "lint": "npx eslint **/*.ts **/*.html",
        "doc": "npx compodoc -p tsconfig.doc.json",
        "update": "ng update @angular/core @angular/cli && ng update",
        "start": "node prebuild.js && ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "build-prod": "node prebuild.js && ng build --configuration=\"production\"",
        "build-staging": "node prebuild.js && ng build --configuration=\"staging\""
    },
    "dependencies": {
        "@angular/animations": "21.0.5",
        "@angular/cdk": "21.0.3",
        "@angular/common": "21.0.5",
        "@angular/compiler": "21.0.5",
        "@angular/core": "21.0.5",
        "@angular/forms": "21.0.5",
        "@angular/material": "21.0.3",
        "@angular/platform-browser": "21.0.5",
        "@angular/platform-browser-dynamic": "21.0.5",
        "@angular/router": "21.0.5",
        "@ngx-translate/core": "17.0.0",
        "@ngx-translate/http-loader": "17.0.0",
        "jwt-decode": "4.0.0",
        "lodash": "4.17.21",
        "moment": "2.30.1",
        "rxjs": "7.8.2",
        "tslib": "2.8.1",
        "zone.js": "0.16.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "21.0.3",
        "@angular/cli": "21.0.3",
        "@angular/compiler-cli": "21.0.5",
        "@eslint/eslintrc": "3.3.3",
        "@types/lodash": "4.17.21",
        "typescript": "5.9.3"
    }
}
